{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "flowtype",
    "compat"
  ],
  "rules": {
    "semi": [2, "never"],
    "no-unexpected-multiline": "off",
    "compat/compat": 2,
    "react/jsx-wrap-multilines": false,
    "react/jsx-uses-react": 1,
    "reast/jsx-uses-vars": 1
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "settings": {
    "polyfills": ["fetch"]
  }
}